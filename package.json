{
  "type": "module",
  "private": true,
  "name": "video-processor",
  "version": "1.0.0",
  "bin": {
    "video-processor": "./dist/src/full.js"
  },
  "scripts": {
    "package": "tsc -p .",
    "install-package": "npm pack --json --pack-destination bin > ./bin/package.json && ts-node-esm scripts/build.ts && npm install -g ./bin/video-processor.tgz",
    "start": "ts-node-esm src/full.ts",
    "test": "ts-node-esm src/test.ts",
    "proccess": "ts-node-esm src/index.ts",
    "stats": "ts-node-esm src/stats.ts",
    "chat": "ts-node-esm src/chatClient.ts",
    "merge": "ts-node-esm src/merge.ts",
    "api": "ts-node-esm src/api.ts",
    "pull": "ts-node-esm src/pull.ts",
    "check": "ts-node-esm src/check.ts"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "csvtojson": "^2.0.10",
    "fluent-ffmpeg": "^2.1.2",
    "json-to-markdown-table": "^1.0.0",
    "json2csv": "^5.0.7",
    "log-update": "^5.0.1",
    "node-fetch": "^3.3.0",
    "ssh2-sftp-client": "^9.0.4"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/json2csv": "^5.0.3",
    "@types/node": "^18.11.18",
    "@types/ssh2-sftp-client": "^9.0.0",
    "node-loader": "^2.0.0",
    "rimraf": "^4.0.4",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  }
}